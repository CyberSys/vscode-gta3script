{
    "name": "gta3script",
    "displayName": "GTA3script",
    "description": "GTA3script support for VSCode",
    "version": "0.0.1",
    "publisher": "thelink2012",
    "license": "MIT",
    "repository": {
        "type" : "git",
        "url" : "https://github.com/thelink2012/vscode-gta3script.git"
    },
    "engines": {
        "vscode": "^1.5.0"
    },
    "categories": [
        "Languages",
        "Snippets"
    ],
    "activationEvents": [
        "onLanguage:gta3script"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [],
        "languages": [
            {
                "id": "gta3script",
                "extensions": [
                    ".sc"
                ],
                "aliases": [
                    "GTA3script"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "gta3script",
                "scopeName": "source.gta3script",
                "path": "./syntaxes/gta3script.tmLanguage"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install"
    },
    "dependencies": {
        "xml2js": "^0.4.17"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    }
}